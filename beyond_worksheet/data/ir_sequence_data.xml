<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="worksheet_sequence" model="ir.sequence">
            <field name="name">Worksheet Sequence</field>
            <field name="code">task.worksheet</field>
            <field name="prefix">SV-%(year)s%(month)s</field>
            <field name="padding">3</field>

            <field name="company_id" eval="False"/>
        </record>
        <record id="rcti_invoice_sequence" model="ir.sequence">
            <field name="name">BILL: RCTI Sequence</field>
            <field name="code">rcti.invoice</field>
            <field name="prefix">RCTI-%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="ccew_sequence" model="ir.sequence">
            <field name="name">CCEW Sequence</field>
            <field name="code">ccew.sequence</field>
            <field name="prefix">CES-%(y)s--</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
